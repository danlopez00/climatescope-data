  Map { background-color: #f2f2f2; }
  #countries [ISO_A2 = "TZ"] {
    ::outline { line-color: #fff; line-width: 2px; }
    polygon-fill: #C3D500;
  }
  #capitals [iso = "TZ"] {
    text-name: [capital:es];
    text-size: 32px;
    text-fill: #333;
    text-halo-fill: #fff;
    text-halo-radius: .75;
    text-face-name: 'Calibri Regular';
    text-dy: -16px;
    marker-width: 12;
    marker-fill: #fff;
    marker-line-color: #333;
    marker-line-width: 2px;
  }
  